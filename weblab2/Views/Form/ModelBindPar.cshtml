
@{
    ViewData["Title"] = "ModelBind";
}

<h1>ModelBind</h1>

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper weblab2.TagHelpers.FieldTagHelper, weblab2
@model weblab2.Models.FormModel
<h1>Index</h1>
<form method="post">
    <div>
        <div><field name="first" value="@Model.first"/></div>
        <div><selector name="operation" default="@Model.operation"></selector></div>
        <div><field name="second" value="@Model.second" /></div>
    </div>
    <input type="submit" value="cals" />
</form>

